#!/usr/bin/env bash
# This script groups visitors by IP and HTTP status code and displays the data

# Use awk to process the log file and print the occurrence number, IP, and HTTP code in the specified format
awk '{ count[$1" "$2]++ } END { for (key in count) print count[key], key }' access.log | awk '{ printf "%6s %15s %s\n", $1, $2, $3 }' | sort -nrk1


